  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for FlappyBird
  pod 'AppCenter'
  pod 'AppCenter/Auth'
  pod 'AppCenter/Data'
  pod 'AppCenter/Crashes'
  pod 'AppCenter/Analytics'
  pod 'AppCenter/Push'

  target 'FlappyBirdTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
